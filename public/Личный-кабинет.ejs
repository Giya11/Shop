      <script> 
      function logout() { 
        $.post("/logout", function(data) { 
          // Обработка ответа от сервера 
          if (data === 'Вы успешно вышли из системы') { 
            window.location.href = '/enter'; // Перенаправление на страницу входа 
          } else { 
            alert('Что-то пошло не так'); 
          } 
        }); 
      } 
      </script>
